{
  "name": "@voiceflow/git-branch-check",
  "description": "Voiceflow branch naming check",
  "version": "1.2.0",
  "author": "Voiceflow",
  "bin": {
    "git-branch-check": "./index.js"
  },
  "bugs": {
    "url": "https://github.com/storyflow/git-branch-check/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "current-git-branch": "^1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@voiceflow/commitlint-config": "^1.0.1",
    "@voiceflow/eslint-config": "^2.0.5",
    "@voiceflow/git-branch-check": "^1.1.3",
    "@voiceflow/prettier-config": "^1.0.5",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.3.1",
    "eslint": "^5.16.0",
    "fixpack": "^4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^1.17.1",
    "prettier-eslint-cli": "^5.0.0"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/storyflow/git-branch-check#readme",
  "keywords": [
    "branch",
    "git",
    "linting",
    "pattern"
  ],
  "license": "MIT",
  "main": "index.js",
  "prettier": "@voiceflow/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storyflow/git-branch-check.git"
  },
  "scripts": {
    "commit": "cz",
    "lint": "fixpack && eslint \"**/*.js\"",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn lint --format json -o sonar/report.json",
    "prepare": "fixpack",
    "test:dependencies": "depcheck"
  }
}
