{
  "name": "@voiceflow/eslint-config",
  "description": "ESLint config for voiceflow",
  "version": "3.2.2",
  "author": "Eric Hacke",
  "bugs": {
    "url": "https://github.com/voiceflow/eslint-config/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.6.0",
    "eslint-plugin-xss": "^0.1.9",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.11.0"
  },
  "devDependencies": {
    "@voiceflow/commitlint-config": "^1.1.0",
    "@voiceflow/eslint-config": "^3.2.1",
    "@voiceflow/git-branch-check": "^1.2.0",
    "@voiceflow/prettier-config": "^1.0.5",
    "commitizen": "^4.2.4",
    "commitlint": "^12.1.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.26.0",
    "fixpack": "^4.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0"
  },
  "files": [
    "*.js",
    "config/*.js",
    "overrides/*.js",
    "!scratch.*"
  ],
  "homepage": "https://github.com/voiceflow/eslint-config#readme",
  "keywords": [
    "config",
    "eslint",
    "lint"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "@voiceflow/prettier-config": "^1.0.2",
    "eslint": "^7.0.0"
  },
  "prettier": "@voiceflow/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/eslint-config.git"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint \"./**/*.{js,ts}\"",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn lint --format json -o sonar/report.json",
    "prepare": "husky install",
    "test": "yarn test:run",
    "test:run": "echo \"Error: no test specified\" && exit 1"
  }
}
