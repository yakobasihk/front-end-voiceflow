{
  "name": "@voiceflow/stylelint-config",
  "description": "Stylelint config for voiceflow",
  "version": "1.0.0",
  "author": "Ben Teichman",
  "bugs": {
    "url": "https://github.com/voiceflow/stylelint-config/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "stylelint-order": "^4.1.0"
  },
  "devDependencies": {
    "@voiceflow/commitlint-config": "^1.1.0",
    "@voiceflow/eslint-config": "^3.2.1",
    "@voiceflow/git-branch-check": "^1.2.0",
    "@voiceflow/prettier-config": "^1.0.5",
    "commitizen": "^4.2.4",
    "commitlint": "^12.1.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "fixpack": "^4.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "stylelint": "^13.13.1"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/voiceflow/stylelint-config#readme",
  "keywords": [
    "config",
    "stylelint",
    "lint"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "stylelint": "^13.13.1"
  },
  "prettier": "@voiceflow/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/stylelint-config.git"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint \"./**/*.{js,ts}\"",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn lint --format json -o sonar/report.json",
    "prepare": "husky install",
    "test": "yarn test:run",
    "test:run": "echo \"Error: no test specified\" && exit 1"
  }
}
